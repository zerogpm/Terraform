# App1 DNS Name
variable "app1_dns_name" {
  description = "App1 DNS Name"
  type        = string
  default = "app1.devopsincloud.com"
}

# App2 DNS Name
variable "app2_dns_name" {
  description = "App2 DNS Name"
  type        = string
  default = "app2.devopsincloud.com"
}

# App default DNS Name
variable "app_dns_name" {
  description = "App2 DNS Name"
  type        = string
  default = "apps.devopeasyway.com"
}

# App default DNS Name
variable "naked_app_dns_name" {
  description = "App2 DNS Name"
  type        = string
  default = "devopeasyway.com"
}